   <extension
         point="org.eclipse.ui.bindings">
         
      <!-- Start from clean slate and define our key configuration - 
		   com.deltawalker.keyConfiguration - because the default key configuration
		   has key bindings for functionality we don't support and some of those key 
		   key bindings we want to use for our own functionality.
		   Using com.deltawalker.keyConfiguration requires that we explicitely [re]define
		    all key bindings we care about, incl. those of the text editor. -->
      <scheme
            name="%keyConfiguration.deltaWalker.name"
            description="%keyConfiguration.deltaWalker.description"
            id="com.deltawalker.keyConfiguration">
      </scheme>
      <!-- win32:  M1=CTRL,    M2=SHIFT, M3=ALT, M4=-
           carbon: M1=COMMAND, M2=SHIFT, M3=ALT, M4=CTRL -->
      <key
            commandId="com.deltawalker.ui.file.compareFiles"
            sequence="M1+M2+F"
            schemeId="com.deltawalker.keyConfiguration"/>
      <key
            commandId="com.deltawalker.ui.file.compareFolders"
            sequence="M1+M2+D"
            schemeId="com.deltawalker.keyConfiguration"/>
      <key
            commandId="com.deltawalker.ui.file.compare"
            sequence="M1+R"
            schemeId="com.deltawalker.keyConfiguration"/>
      <key
            commandId="com.deltawalker.ui.file.cancel"
            sequence="ESC"
            schemeId="com.deltawalker.keyConfiguration"/>
      <key
            commandId="com.deltawalker.ui.file.open"
            sequence="M1+O"
            schemeId="com.deltawalker.keyConfiguration"/>
      <key
            commandId="org.eclipse.ui.file.close"
