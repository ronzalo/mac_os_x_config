<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="Application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.deltawalker.product.application.internal.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="%perspective.name"
            class="com.deltawalker.product.application.internal.DeltaWalkerPerspective"
            id="com.deltawalker.product.application.deltaWalkerPerspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.activities">
      <activity
            id="com.deltawalker.activities.update"
            name="Disable P2 Update Menus">
      </activity>
      <activityPatternBinding
         activityId="com.deltawalker.activities.update"
         pattern="org\.eclipse\.equinox\.p2\.ui..*/.*">
      </activityPatternBinding>
   </extension>
   <extension
         id="deltaWalker"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.deltawalker.product.application.Application"
            name="%product.name">
         <property
               name="preferenceCustomization"
               value="preferences.ini"/>
         <property
               name="aboutImage"
               value="icons/about.png"/>
<!--
Contrary to what deltaWalker.product says, the size and position of the
startup message can only be controlled from here. Same for the message's 
foreground color:
          <property
                name="startupForegroundColor"
                value="C8D5EA"/>
For now just get rid of the startup message by giving its rectangle 0 size.
-->                
          <property
                name="startupMessageRect"
                value="0,0,0,0"/>
          <property
                name="startupProgressRect"
                value="0,280,455,15"/> 
         <property
               name="windowImages"
               value="icons/DeltaWalker16x16.png,icons/DeltaWalker32x32.png,icons/DeltaWalker48x48.png,icons/DeltaWalker64x64.png,icons/DeltaWalker128x128.png"/>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.intro">
      <intro
            icon="platform:/plugin/com.deltawalker.product.application/icons/DeltaWalker16x16.png"
            class="org.eclipse.ui.intro.config.CustomizableIntroPart"
            id="com.deltawalker.product.application.intro"/>
      <introProductBinding
            introId="com.deltawalker.product.application.intro"
            productId="com.deltawalker.product.application.deltaWalker"/>
   </extension>
   <extension
         point="org.eclipse.ui.intro.config">
      <config
            content="intro/IntroContent.xml"
            id="com.deltawalker.product.application.introConfigId"
            introId="com.deltawalker.product.application.intro">
         <presentation home-page-id="root">
            <implementation
                  kind="html"
                  os="win32,linux,macosx"
                  style="intro/css/shared.css"/>
         </presentation>
      </config>
   </extension>
